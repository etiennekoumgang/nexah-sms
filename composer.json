{
    "name": "gladdle/nexah-sms",
    "description": "A php library for sending SMS to one or more phone numbers using the Nexah API",
    "keywords": ["sms", "nexah"],
    "homepage": "https://github.com/etiennekoumgang/nexah-sms",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Gladdle\\NexahSms\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Gladdle\\NexahSms\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Koume KOUMGANG",
            "email": "etiennekoumgang@gmail.com",
            "homepage": "https://www.linkedin.com/in/etiennekoumgang/"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "^7.4 || ^8.0",
        "symfony/http-client": ">=5.0 <8.0",
        "monolog/monolog": ">=2.0 <4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.6",
        "phpstan/phpstan": "^2.0"
    }
}
